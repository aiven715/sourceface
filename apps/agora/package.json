{
  "name": "agora",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon --exec 'babel-node ./src/index.js'",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\" && eslint \"src/**/*.js\"",
    "build": "babel ./src --out-dir ./build",
    "test": "",
    "migrate:create": "migrate create",
    "migrate:list": "migrate list --store='./migration-store'",
    "migrate:up": "migrate up --store='./migration-store'",
    "migrate:down": "migrate down --store='./migration-store'",
    "postinstall": "yarn migrate:up"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^6.2.1",
    "core-js": "^3.6.4",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^4.0.7",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "pg-promise": "^10.4.4",
    "ramda": "^0.27.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "migrate": "^1.6.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2"
  }
}
